{% block menu %}
  <ul class="d-flex list-unstyled">
    {% block items %}
      {% for item in items %}
        {% block item %}
          <li class="nav-item px-1">
            {{ link(item.title, item.url, {'class': ['nav-link']}) }}
          </li>
        {% endblock %}
      {% endfor %}
    {% endblock %}
  </ul>
{% endblock %}

